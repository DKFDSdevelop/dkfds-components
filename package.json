{
  "name": "dkfds",
  "version": "9.0.2",
  "description": "Det Fælles Designsystem er den fælles guide til selvbetjeningsløsninger, der skal på Borger.dk og Virk.",
  "engines": {
    "node": ">= 4"
  },
  "main": "dist/js/dkfds.min.js",
  "scripts": {
    "build": "webpack",
    "lint-css": "npx stylelint \"dist/css/dkfds.css\"",
    "lint-scss": "npx stylelint \"src/stylesheets/**\" --config stylelint-scss.json --formatter verbose",
    "lint-all-css": "npx stylelint \"dist/css/dkfds-borgerdk.css\" \"dist/css/dkfds-virkdk.css\" \"dist/css/dkfds.css\"",
    "css-only": "sass --no-source-map --style=expanded src/stylesheets/dkfds.scss:css-only/dkfds.css src/stylesheets/dkfds-virkdk.scss:css-only/dkfds-virkdk.css src/stylesheets/dkfds-borgerdk.scss:css-only/dkfds-borgerdk.css"
  },
  "author": "Erhvervsstyrelsen og Digitaliseringsstyrelsen",
  "license": "(MIT AND OFL-1.1 AND Apache-2.0)",
  "devDependencies": {
    "array-foreach": "^1.0.2",
    "classlist-polyfill": "^1.0.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.52.0",
    "eslint-webpack-plugin": "^4.0.1",
    "mini-css-extract-plugin": "^2.7.6",
    "normalize.css": "^8.0.1",
    "object-assign": "^4.1.1",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.2.0",
    "receptor": "^0.1.3",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "stylelint": "^15.11.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "stylelint-no-unsupported-browser-features": "^7.0.0",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-remove-empty-scripts": "^1.0.4"
  }
}

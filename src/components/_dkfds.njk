<!doctype html>
<html lang="da">
  <head>
    <title>{{ _self.title }}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Vendor example stylesheets (Select2 and Datatables)-->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.5/css/select2.css">
    <link rel="stylesheet" href="https://cdn.datatables.net/1.10.19/css/jquery.dataTables.min.css">
    <link rel="stylesheet" href="https://cdn.datatables.net/responsive/2.2.3/css/responsive.dataTables.min.css">
    <link rel="stylesheet" href="https://cdn.datatables.net/select/1.2.7/css/select.dataTables.min.css">

    <!-- Frontend styleguide stylesheet-->
    <link rel="stylesheet" href="{{ dkfds.path }}/css/dkfds.min.css">
  </head>
  <body>
    <div class="hide-base-svg">
      {% include "all-svg-icons.svg"  %}
    </div>
    {% block body %}{{ yield }}{% endblock %}
    <script src="{{ dkfds.path }}/js/dkfds.js"></script>
    <script>
    window.addEventListener('load', function() {
      document.body.addEventListener('submit', function(e) {
        // https://github.com/18F/web-design-standards/issues/2101
        e.preventDefault();
      }, true);
    }, false);
    </script>
  </body>
</html>



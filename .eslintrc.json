{
    "root": true,
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "no-duplicate-imports": "warn",
        "no-unused-vars": "warn",
        "eqeqeq": "warn",
        "no-console": "error"
    }
}
